{
   "version":"2",
   "templates":[
      {
         "image":"pihole/pihole:latest",
         "logo":"https://raw.githubusercontent.com/SelfhostedPro/selfhosted_templates/master/Images/pihole.png",
         "name":"pihole",
         "platform":"linux",
         "ports":[
            "53:53/tcp",
            "53:53/udp",
            "67:67/udp",
            "8080:80/tcp",
            "4443:443/tcp"
         ],
         "restart_policy":"unless-stopped",
         "title":"Pi-Hole",
         "type":1,
         "volumes":[
            {
               "bind":"/portainer/Files/AppData/Config/PiHole",
               "container":"/etc/pihole"
            },
            {
               "bind":"/portainer/Files/AppData/Config/PiHole/DNS",
               "container":"/etc/dnsmasq.d"
            }
         ]
      },
      {
         "logo":"https://raw.githubusercontent.com/SelfhostedPro/selfhosted_templates/master/Images/nextcloud-icon.png",
         "name":"nextcloud",
         "platform":"linux",
         "repository":{
            "stackfile":"nextcloud.yml",
            "url":"https://github.com/RioT-LiNaRo/PortainerCustomTemplate"
         },
         "title":"Nextcloud",
         "type":1
      },
      {
         "env":[
            {
               "label":"ADMIN_TOKEN",
               "name":"ADMIN_TOKEN",
               "set":""
            }
            ],
         "image":"bitwardenrs/server:latest",
         "logo":"https://raw.githubusercontent.com/SelfhostedPro/selfhosted_templates/master/Images/bitwarden.png",
         "name":"bitwardenrs",
         "platform":"linux",
         "ports":[
            "32774:80/tcp"
         ],
         "restart_policy":"unless-stopped",
         "title":"Bitwarden RS",
         "type":1,
         "volumes":[
            {
               "bind":"/portainer/Files/AppData/Config/Bitwarden-rs",
               "container":"/config"
            }
         ]
      },
      {
         "env":[
            {
               "label":"SUBDOMAINS",
               "name":"SUBDOMAINS",
               "set":""
            },
            {
               "label":"TOKEN",
               "name":"TOKEN",
               "set":""
            },
            {
               "default":"1000",
               "label":"PUID",
               "name":"PUID"
            },
            {
               "default":"100",
               "label":"PGID",
               "name":"PGID"
            }
         ],
         "image":"linuxserver/duckdns:latest",
         "logo":"https://raw.githubusercontent.com/SelfhostedPro/selfhosted_templates/master/Images/duckdns.png",
         "name":"duckdns",
         "platform":"linux",
         "restart_policy":"unless-stopped",
         "title":"Duck DNS",
         "type":1
      },
      {
         "image":"jc21/nginx-proxy-manager:latest",
         "logo":"https://raw.githubusercontent.com/jc21/nginx-proxy-manager/master/frontend/app-images/logo-256.png",
         "name":"nginx-proxy-manager",
         "platform":"linux",
         "restart_policy":"unless-stopped",
         "ports":[
            "80:80/tcp",
            "81:81/tcp",
            "443:443/tcp"
         ],
         "title":"Nginx Proxy Manager",
         "type":1,
         "volumes":[
            {
               "bind":"/portainer/Files/AppData/Config/nginx/config.json",
               "container":"/app/config/production.json"
            },
            {
               "bind":"/portainer/Files/AppData/Config/nginx/data",
               "container":"/data"
            },
            {
               "bind":"/portainer/Files/AppData/Config/nginx/letsencrypt",
               "container":"/etc/letsencrypt"
            }
         ]
      },
      {
         "env":[
         {
               "default":"1000",
               "label":"PUID",
               "name":"PUID"
          },
         {
               "default":"100",
               "label":"PGID",
               "name":"PGID"
         },
            {
               "label":"TZ",
               "name":"TZ"
            },
            {
               "label":"DATABASE_PASSWORD",
               "name":"DATABASE_PASSWORD"
            },
            {
               "label":"MYSQL_ROOT_PASSWORD",
               "name":"MYSQL_ROOT_PASSWORD"
            },
            {
               "label":"MYSQL_DATABASE1",
               "name":"MYSQL_DATABASE1"
            },
            {
               "label":"MYSQL_DATABASE2",
               "name":"MYSQL_DATABASE2"
            },
            {
               "label":"MYSQL_USER",
               "name":"MYSQL_USER"
            }
         ],
         "image":"yobasystems/alpine-mariadb:latest",
         "logo":"https://raw.githubusercontent.com/SelfhostedPro/selfhosted_templates/master/Images/mariadb-icon.png",
         "name":"database",
         "platform":"linux",
         "ports":[
            "3306:3306/tcp"
         ],
         "restart_policy":"unless-stopped",
         "title":"DataBase",
         "type":1
      }
   ]
}
